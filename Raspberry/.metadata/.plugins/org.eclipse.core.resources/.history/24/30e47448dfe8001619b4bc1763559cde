package devices;

import events.Events;
import events.PossibleEvents;
import observer.Observable;
import observer.Observer;

public class ButtonObserver implements Observer {

	public ButtonObserver() {
	}
	
	@Override
	public void update(final Observable obs,final Events ev) {
		if (obs instanceof ObservableButton) {
			switch (ev.getEvents().toString()) {
			case ""+PossibleEvents.IS_PRESSED:
				
				break;

			default:
				break;
			}
		}
	}

}
